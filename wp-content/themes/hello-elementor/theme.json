{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"color": {
			"custom": true,
			"link": true
		},
		"typography": {
			"customFontSize": true,
			"lineHeight": true,
			"fontFamilies": [
				{
					"fontFamily": "helvetica-neue-r",
					"slug": "helvetica-neue-r",
					"fontFace": [
						{
							"fontFamily": "helvetica-neue-r",
							"fontStretch": "",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": "http://localhost/Adrian_Gutierrez_Portfolio_Website/wp-content/uploads/useanyfont/1747-Helvetica-Neue-R.woff2"
						}
					],
					"isUAF": true
				}
			]
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"margin": true,
			"padding": true,
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		}
	}
}